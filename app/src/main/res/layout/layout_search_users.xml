<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <TextView
        android:id="@+id/name_search_user"
        android:layout_width="330dp"
        android:layout_height="35dp"
        android:textSize="20sp"
        android:textColor="@color/strings"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginStart="50dp"
        />

    <TextView
        android:id="@+id/nif_search_user"
        android:layout_width="330dp"
        android:layout_height="25dp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/name_search_user"
        android:layout_marginStart="80dp"
        />

    <TextView
        android:id="@+id/genero_search_users"
        android:layout_width="330dp"
        android:layout_height="25dp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/nif_search_user"
        android:layout_marginStart="80dp"
        />

    <TextView
        android:id="@+id/tipoUser_search_user"
        android:layout_width="330dp"
        android:layout_height="25dp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/genero_search_users"
        android:layout_marginStart="80dp"
        />

    <TextView
        android:id="@+id/email_search_user"
        android:layout_width="330dp"
        android:layout_height="25dp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tipoUser_search_user"
        android:layout_marginStart="80dp"
        />


</androidx.constraintlayout.widget.ConstraintLayout>